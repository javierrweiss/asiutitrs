/**
 * @properties={typeid:24,uuid:"C668E36D-2906-49F5-8240-ACF9E4C4D714"}
 * @AllowToRunInFind
 */
function sub_buildReport() {
	var mes = application.getServerTimeStamp().getMonth()    
	
	frm_fecha_txt = 'Ciudad Autónoma de Buenos Aires, ' + application.getServerTimeStamp().getDate() + ' de ' + globals.Traer_nom_mes_corto(mes)
	              + ' del ' + application.getServerTimeStamp().getFullYear() + '.-'
    frm_campoHTML = ''
	var html = ''
	html += '<html><body>'
	html += '<br>'
	html += '<br>'
	html += '<br>'
	html += '<br>'
	html += '<p align="justify">En el día de la fecha he recibido el proceso de Inducción a la Institución y he realizado la recorrida con el objetivo de conocer los distintos sectores del Sanatorio Colegiales.</p>'
	html += '<br>'
	html += '<br>'
	html += '<br>'
	html += '<br>'
	html += '<table width=40% cellpadding=0 cellspacing=0>'
	html += '<tr>'
	html += '<td width=30% align="left">Firma</td>'
	html += '<td width=70% align="right">_______________________________________</td>'
	html += '</tr>'
	html += '</table>'
	html += '<br>'	
	html += '<table width=40% cellpadding=0 cellspacing=0>'
	html += '<tr>'
	html += '<td width=30% align="left">Aclaración</td>'
	html += '<td width=70% align="right">___________________________________</td>'
	html += '</tr>'
	html += '</table>'
	
			
	html += '</body></html>'	
	frm_campoHTML = html	
	
}
