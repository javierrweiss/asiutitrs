 */
function printRoutine(formname, rec) {
	gbl_Form = formname;
	gbl_orienta = 1
	var oneRecord = rec;

	if(!oneRecord) 
	{
	    oneRecord = false
	}	

	if(!gbl_Form) return;
	
	var win = application.createWindow("prn",JSWindow.MODAL_DIALOG)	
	win.title= ' '
	win.resizable = true
	win.setSize(1300,800)
	solutionModel.removeForm
	win.show(forms.prn_previous)
	win.hide()
	win.destroy()	
	//forms[frm].controller.showPrintPreview(oneRecord)
	//forms.impr_html.controller.showPrintPreview()
	//application.showForm(forms[frm])
	/////forms[frm].controller.print(oneRecord,false)
	//forms.impre.controller.showPrintPreview(oneRecord)
}

/**
 * 
 * @param args
 *
 * @properties={typeid:24,uuid:"E0CF6783-3DA8-4317-B90F-2D022FBFD1F7"}
 */
function llamada_desde_Menu(args) {
	
	// Trae datos desde el Menu
	args = args.toString()
	args=args.split(';') 
	globals.vLegajo=utils.stringToNumber(args[0])
	globals.vLega=utils.stringToNumber(args[1])
	globals.vOperador=args[2]
	globals.vTipoOperador=utils.stringToNumber(args[3])
	globals.gbl_transacciones_1=args[4]
	globals.vEmpresa=args[5]
	globals.gbl_expanding_Nodes=args[6]
	globals.gbl_title = ' '
	
	//Muestra Formulario Principal 
	if(globals.gbl_transacciones_1 != 0){	    	
	    switch (globals.gbl_transacciones_1){
		    case 1:case 2:case 3:case 4:case 6:case 7:case 10:case 11:
		    case 12:case 13:case 14:case 15:case 16:
			    var win = application.createWindow("Menu_itm",JSWindow.MODAL_DIALOG)	
	            win.title= globals.gbl_title
	            win.resizable = false
	            win.show(forms.frm_consentimientos_hist); break;
		    case 5: case 8: case 18: case 19: case 20: case 21: case 22: 
		        globals.hisclin = 0
		        globals.gbl_hiscli = 0
		        forms.frm_consentimientos_hist.procesar(); break;
		    case 23:
		        win = application.createWindow("Menu_itm",JSWindow.MODAL_DIALOG)	
                win.title= globals.gbl_title
                win.resizable = false
                win.show(forms.frm_consentimientos_hist_tipo); break;	        
	    }
	}
}

