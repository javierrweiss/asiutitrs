function onAction_scroll_nomencla_anatopat(event) {
	globals.vNomencla=forms.frm_scroll_nomencla_anatopat.nome_codigo
	//var win = application.getWindow()
	//application.closeAllWindows();
	var name=application.getActiveWindow()
	name.getName()
	var $win=application.getWindow(name.getName())
	if($win!=null){
		$win.hide()
		$win.destroy()
	}
}

