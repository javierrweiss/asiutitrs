/**
 * Callback method for when form is shown.
 *
 * @param {Boolean} firstShow form is shown first time after load
 * @param {JSEvent} event the event that triggered the action
 *
 * @properties={typeid:24,uuid:"CA2058E2-024A-4C9E-9A25-2C721F76F396"}
 * @AllowToRunInFind
 */
function onShow_inicializa_var(firstShow, event) {
	foundset.find()
	foundset.mov_nro_doc=forms.frm_movimientos_tbl.foundset.mov_nro_doc
	foundset.mov_fecha_ing=forms.frm_movimientos_tbl.foundset.mov_fecha_ing
	foundset.mov_hora_ing=forms.frm_movimientos_tbl.foundset.mov_hora_ing
	foundset.search()
}
