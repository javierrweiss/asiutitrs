/**
 * Perform the element right-click action.
 *
 * @param {JSEvent} event the event that triggered the action
 *
 * @properties={typeid:24,uuid:"6DA767B7-8EC4-4435-954C-C6019A66EFA9"}
 */
function onRightClickNewRegister(event) {
	// TODO Auto-generated method stub
	// foreign key data is only filled in for equals (=) relation items
	elements.tabs.tabIndex=2;
	var idx = currentcontroller.newRecord();//default adds on top
	//currentcontroller.newRecord(false); //adds at bottom
	//currentcontroller.newRecord(2); //adds as second record
	if(idx>=0){
		forms.frm_admision_dtl.foundset.adm_apelnom = "";
		forms.frm_admision_dtl.foundset.adm_cama = "";
		forms.frm_admision_dtl.foundset.adm_habita ="";
		forms.frm_admision_dtl.foundset.adm_fecnac = "";
		forms.frm_admision_dtl.foundset.adm_fecaltaefec = "";
		forms.frm_admision_dtl.foundset.adm_fecing = "";
		forms.frm_admision_dtl.foundset.adm_obrsoc = "";
		forms.frm_admision_dtl.foundset.adm_sexo = "";
	}
	
}
