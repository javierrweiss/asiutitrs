function estadopant()
{
	if (reservasesta == 1 )
	{	
		if (reservasatendi == 0 )   //  0  , 3 y 4
			{estadopant = "Sin atender"}
		else 
			{if (reservasatendi == 3 )
				{estadopant = "At.en espera"}
			else
				{estadopant = "Tomado"}	
			}
	}
	else	
	{estadopant = "Reservado"}
}
