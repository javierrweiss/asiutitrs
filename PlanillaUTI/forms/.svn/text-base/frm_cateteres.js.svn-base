 *
 * @properties={typeid:24,uuid:"C2168B4E-D7A5-4074-8F41-5725C84CA9F1"}
 */
function onAction_f1(event) {
	var win = application.createWindow("c",JSWindow.MODAL_DIALOG)
	win.title = globals.vTituloVentana;
	win.setSize(700,380)
	win.show(forms.tab_cronologia_cateteres)
}

