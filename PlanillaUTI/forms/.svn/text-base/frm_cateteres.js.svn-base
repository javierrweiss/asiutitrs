
/**
 * Callback method for when form is shown.
 *
 * @param {Boolean} firstShow form is shown first time after load
 * @param {JSEvent} event the event that triggered the action
 *
 * @properties={typeid:24,uuid:"257E6C9C-BC1F-48EB-B187-83B600CED7C2"}
 */
function onShow_frm_cateteres(firstShow, event) {
	if(globals.vNuevaHoja){
		globals.HabilitaDeshabilitaCampos("frm_cateteres","nueva")
	}else{
		globals.HabilitaDeshabilitaCampos("frm_cateteres","consulta")
	}
}

