 *
 * @properties={typeid:24,uuid:"182B4A64-5052-466F-A600-99237893B834"}
 */
function onAction_f1_viacen(event) {
	globals.vTipoCateteres='01viacen'
	globals.vTituloVentana="Cronología de Vía Central";
	onAction_f1(event)
}

/**
 * Perform the element default action.
